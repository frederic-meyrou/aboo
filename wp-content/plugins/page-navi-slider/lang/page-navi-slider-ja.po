msgstr "プレビュー"

msgid "Settings"
msgstr "設定"

msgid "Reset to default"
msgstr "デフォルトにリセット"

msgid "Preview options"
msgstr "プレビューオプション"

msgid "General settings"
msgstr "全般設定"

msgid "Caption"
msgstr "見出し"

msgid "Fonts"
msgstr "フォント"

msgid "Colors"
msgstr "色"

msgid "Borders"
msgstr "ボーダー"

